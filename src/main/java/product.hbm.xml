<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.productdetails.ProductDetails" table="producttable">
        <id name="product_Id" type="integer" column="ID">
            <generator class="native" />
        </id>
        <property name="product_Name" type="string" column="NAME" not-null="true" />
        <property name="product_Price" type="double" column="PRICE" not-null="true" />
        <property name="productType" type="string" column="TYPE" not-null="true" />
        <property name="productQuantity" type="integer" column="QUANTITY" not-null="true" />

    </class>
    
</hibernate-mapping>
